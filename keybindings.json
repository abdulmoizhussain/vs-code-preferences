  "window.zoomLevel": 0,
  "workbench.startupEditor": "none",
  "files.autoSaveDelay": 10000,
  "editor.renderWhitespace": "all",
  "editor.minimap.enabled": true,
  "editor.fontFamily": "'lucida sans typewriter', 'consolas', 'Fira Mono', 'Courier New'",
  "editor.fontLigatures": true,
  "editor.emptySelectionClipboard": true,
  "editor.mouseWheelZoom": true,
  "editor.detectIndentation": false,
  "editor.hover.enabled": true,
  "editor.hover.delay": 1000,
  "editor.autoIndent": "full",
  "editor.trimAutoWhitespace": false,
  "editor.wordWrapColumn": 300,
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "extensions.ignoreRecommendations": true,
  "update.showReleaseNotes": false,
  "window.openFoldersInNewWindow": "on",
  "html.format.wrapAttributes": "auto",
  "html.format.wrapLineLength": 300,
  "html.format.enable": true,
  "html.format.preserveNewLines": true,
  "html.format.maxPreserveNewLines": null,
  "html.format.endWithNewline": false,
  // "html.format.extraLiners": "head, body, /html",
  "html.format.extraLiners": null,
  "emmet.includeLanguages": {
      "javascript": "javascriptreact",
      "vue-html": "html",
      "razor": "html",
      "plaintext": "jade"
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "python.jediEnabled": false,
  "telemetry.enableTelemetry": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true,
  },
  //"editor.snippetSuggestions": "",
  "editor.suggestSelection": "first",
  "emmet.showExpandedAbbreviation": "never",
  // "[html]": {
  //   "editor.defaultFormatter": "HookyQR.beautify"
  // },
  // "window.title": "${dirty}${activeEditorShort}${separator}${rootPath}${separator}${rootName}${separator}${appName}",
  "window.title": "${rootPath}${separator}${dirty}${activeEditorShort}",
  "window.titleBarStyle": "custom",
  "editor.tabSize": 2
}
